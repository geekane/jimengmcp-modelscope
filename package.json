{
  "name": "jimeng-mcp",
  "version": "1.6.1",
  "description": "MCP服务器项目，集成即梦AI图像生成",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./server": {
      "types": "./lib/server.d.ts",
      "import": "./lib/server.js",
      "require": "./lib/server.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "repository": "git@github.com:c-rick/jimeng-mcp.git",
  "author": "rick <1196363729@qq.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node -r dotenv/config lib/index.js",
    "start:dev": "nodemon -r dotenv/config lib/index.js",
    "type-check": "tsc --noEmit",
    "test": "npx @modelcontextprotocol/inspector node -r dotenv/config ./lib/index.js"
  },
  "devDependencies": {
    "@types/node": "^22.15.2",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.10",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.2",
    "@types/crypto-js": "^4.2.2",
    "@volcengine/openapi": "^1.30.1",
    "axios": "^1.9.0",
    "crc32": "^0.2.2",
    "crypto": "1.0.1",
    "crypto-js": "4.2.0",
    "dotenv": "^16.5.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  }
}
